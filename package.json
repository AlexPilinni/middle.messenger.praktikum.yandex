{
  "name": "middle.yandex.praktikum.messenger",
  "version": "1.0.0",
  "description": "messenger",
  "main": "index.js",
  "scripts": {
    "start": "parcel src/pages/index/index.pug --open",
    "build": "parcel build",
    "server": "node ./server/app.js"
  },
  "compilerOptions": {
    "baseUrl": "."
  },
  "targets": {
    "frontend": {
      "source": [
        "./src/pages/login/login.pug",
        "./src/pages/signin/signin.pug",
        "./src/pages/profile/profile.pug",
        "./src/pages/messenger/messenger.pug",
        "./src/pages/edit-password/edit-password.pug",
        "./src/pages/edit-profile/edit-profile.pug",
        "./src/pages/404/404.pug",
        "./src/pages/500/500.pug",
        "./src/pages/index/index.pug"
      ],
      "distDir": "./dist"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexPilinni/middle.yandex.praktikum.messenger.git"
  },
  "keywords": [
    "messenger",
    "yandex",
    "praktikum"
  ],
  "author": "Alex Pilin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AlexPilinni/middle.yandex.praktikum.messenger/issues"
  },
  "homepage": "https://github.com/AlexPilinni/middle.yandex.praktikum.messenger#readme",
  "devDependencies": {
    "@parcel/transformer-pug": "^2.6.2",
    "@parcel/transformer-sass": "^2.6.2",
    "express": "^4.18.1",
    "parcel": "^2.6.2",
    "path": "^0.12.7"
  }
}
